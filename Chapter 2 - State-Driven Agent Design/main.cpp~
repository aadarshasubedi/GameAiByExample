#include "Miner.h"

#define MAX_ITERATIONS 100

int main (int narg, char const* args[])
{
    Miner miner = new Miner(13);

    for (unsigned int i = 0; i < 100; ++i)
    {
        miner.Update();
    }
    
    return 0;
}
